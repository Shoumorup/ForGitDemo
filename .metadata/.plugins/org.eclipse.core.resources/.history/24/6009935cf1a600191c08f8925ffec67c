package com.visa.training.jdbc;

public class ProductCreate {
	
	public static void main(String[] args)throws Exception {
		ProductDAO pdao = new ProductDAO();
		Product p = new Product(1,"AAA",12.5f,3);
		pdao.save(p);
		Product p2 = new Product(2,"BBB",45.0f,12);
		pdao.save(p2);
		Product p3 = new Product(3,"CCC",23.3f,8);
		pdao.save(p3);
	}

}
